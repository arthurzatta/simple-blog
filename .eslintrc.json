{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-params-reassign": "off",
        "no-unused-vars": ["error", {"argsIgnorePattern": "next"}],
        "import/extensions" :[
          "error",
          "ignorePackages",
          {
            "js" : "never"
          }
        ]
    }
}
